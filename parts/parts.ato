component XT60PW_F:
	"""Female XT60 (supplies power), with right angle pins."""
	signal gnd ~ pin 1
	signal vin ~ pin 2
	power = new ElectricPower
	power.vcc ~ vin
	power.gnd ~ gnd

	lcsc_id = "C428722"
	designator_prefix = "J"

component XT60PW_M:
	"""Male XT60 (receives power), with right angle pins."""
	signal gnd ~ pin 1
	signal vin ~ pin 2
	power =  new ElectricPower
	power.vcc ~ vin
	power.gnd ~ gnd

	lcsc_id = "C98732"
	designator_prefix = "J"

