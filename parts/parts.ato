	signal S

	pin 1 ~ S
	pin 2 ~ S
	pin 3 ~ S
	pin 4 ~ G
	pin 5 ~ D
	pin 6 ~ D
	pin 7 ~ D
	pin 8 ~ D
	pin 9 ~ D

